{
  "build": {
    "content": [

      {"files": ["**/*.yml", "**/*.md"], "version": "sharepoint-ps", "src": "docs-conceptual", "dest": "sharepoint" },        
      {"files": ["**/*.yml"], "src": "sharepoint-ps", "version": "sharepoint-ps", "exclude": ["docs-conceptual"], "dest": "module"},

      {"files": ["**/*.yml", "**/*.md"], "version": "sharepoint-server-ps", "src": "docs-conceptual", "dest": "sharepoint" },
      {"files": ["**/*.yml"], "src": "sharepoint-server-ps", "version": "sharepoint-server-ps", "exclude": ["docs-conceptual"], "dest": "module"}

    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "versions": {
      "sharepoint-server-ps": {
        "dest": "sharepoint-server-ps"
      },
      "sharepoint-ps": {
        "dest": "sharepoint-ps"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/powershell/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/OfficeDocs-SharePoint-powershell",
      "feedback_product_url": "https://github.com/MicrosoftDocs/OfficeDocs-SharePoint-powershell/issues",
      "author": "techwriter40",
      "ms.author": "pamgreen",
      "ms.devlang": "powershell",
      "ms.prod": "sharepoint-server-itpro"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "sharepointps"
  }
}
